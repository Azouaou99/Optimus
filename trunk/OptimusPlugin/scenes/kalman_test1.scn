<?xml version="1.0" ?>
<Node name="root" gravity="0 -9.81 0" dt="0.01">
    <RequiredPlugin name="Kalman Filter plugin" pluginName="Kalman" />

    <VisualStyle displayFlags="showVisualModels showBehaviorModels hideCollisionModels hideMappings hideForceFields" />

    <Node name="Ball" >
        <EulerImplicitSolver vdamping="0.0" />
        <CGLinearSolver iterations="25" name="linear solver" tolerance="1.0e-9" threshold="1.0e-9"/>
        <MechanicalObject name="DOFs" template="Rigid" position="0 0 0  0 0 0 1" />
        <UniformMass name="mass" totalMass="1" />
        <Node name="Visual" >
            <OglModel name="ball" filename="ball.obj" scale3d="0.01 0.01 0.01" />
            <RigidMapping template="Rigid" name="rigidMapping" input="@../DOFs" output="@ball"/>
        </Node>
    </Node>

    <Node name="Kalman"  >
        <KalmanFilter position="@../Ball/DOFs.position" />
    </Node>

</Node>
